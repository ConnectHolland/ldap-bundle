{
    "name": "connectholland/ldap-bundle",
    "description": "LDAP authentication for various existing user bundles.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Nijens",
            "email": "niels@connectholland.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "ConnectHolland\\LdapBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConnectHolland\\LdapBundle\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/ldap": "^2.8 || ^3.0",
        "symfony/property-access": "^2.8 || ^3.0",
        "symfony/security": "^2.8 || ^3.0",
        "symfony/security-bundle": "^2.8 || ^3.0"
    },
    "require-dev": {
        "jackalope/jackalope": "^1.3",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "matthiasnoback/symfony-dependency-injection-test": "^1.1",
        "phpunit/phpunit": "^5.7",
        "satooshi/php-coveralls": "^1.0",
        "sulu/sulu": "^1.4",
        "symfony/symfony": "^2.8 || ^3.0"
    },
    "config": {
        "sort-packages": true
    }
}
